INSERT INTO `mobster` VALUES (1, 'Samsung', 'Galaxy M14', 12000, 30),
(2, 'Redmi', 'Note 12', 15000, 25),
(3, 'Realme', 'Narzo 50', 13000, 20),
(4, 'Samsung', 'Galaxy A23', 18000, 10);

SELECT * FROM `mobster` WHERE price>13000 OR Stock<15;

UPDATE `mobster` SET `Price`=12500,`Stock`= (Stock+5) WHERE Model='Narzo 50';

DELETE FROM `mobster` WHERE id=2;

SELECT MIN(Price),MAX(Price) FROM `mobster`;

SELECT SUM(Stock) FROM `mobster`;

SELECT * FROM `mobster` ORDER BY Price DESC LIMIT 2;